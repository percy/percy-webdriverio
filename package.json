{
  "name": "@percy/webdriverio",
  "version": "1.0.4",
  "description": "percy.io integration for webdriver.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist/",
    "build": "npm run clean && tsc",
    "pretest": "npm run build",
    "test": "percy exec -- wdio wdio.conf.js"
  },
  "dependencies": {
    "@percy/agent": "~0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@wdio/cli": "^6.4.0",
    "@wdio/local-runner": "^6.4.0",
    "@wdio/mocha-framework": "^6.4.0",
    "@wdio/selenium-standalone-service": "^6.4.0",
    "@wdio/spec-reporter": "^6.4.0",
    "chai": "^4.2.0",
    "http-server": "^0.12.3",
    "mocha": "^8.0.1",
    "semantic-release": "^17.0.4",
    "typescript": "^4.0.3",
    "webdriverio": "^6.1.0"
  },
  "resolutions": {
    "ecstatic": "3.2"
  },
  "author": "Perceptual Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/percy/percy-webdriverio/issues"
  },
  "repository": "https://github.com/percy/percy-webdriverio",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
